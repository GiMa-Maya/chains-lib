{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {},
    "test:watch": {
      "cache": false
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "watch": {
      "cache": false,
      "outputs": []
    },
    "typedoc": {
      "outputs": []
    }
  }
}